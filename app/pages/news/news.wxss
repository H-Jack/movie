page,text,view,navigator,swiper,swiper-item,image{
  margin: 0px;
  padding: 0px;
  box-sizing: border-box;
}
.nav {
  width: 100%;
  height: 220rpx;
}
.smallBox {
  width: 100%;
  height: 220rpx;
  border-bottom: 1rpx solid #d4d4d4;
}
.content {
  width: 68%;
  height: 100%;
  float: left;
  padding-left: 15rpx;
  padding-top: 20rpx;
}
.texts {
  width: 100%;
  height: 75%;
  padding-top: 5rpx;
}
.texts text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.address {
  width: 100%;
  height: 25%;
  font-size: 25rpx;
}
.images {
  width: 30%;
  height: 100%;
  float: right;
  overflow: hidden;
  padding: 10rpx;
}
.images image {
  width: 100%;
}